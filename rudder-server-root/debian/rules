#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

# The root Makefile has all possible target and prevent dh to autodetect them
# We have no test
override_dh_auto_test:

override_dh_gencontrol:
	dh_gencontrol -- "-Vrudder:deps=$(shell ../format-dependencies dpkg $(shell dpkg-parsechangelog -SVersion) rudder-webapp rudder-reports rudder-agent)"

# Default rule that guess what to do for missing targets
%:
	dh $@
